RewriteBase /
RewriteEngine On
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}.html -f
RewriteRule ^(.*)$ $1.html
RewriteCond %{SERVER_PORT} 80
RewriteCond %{HTTP_HOST} ^(www\.)?grossyfatties\.com
RewriteCond %{HTTP_HOST} ^(\.)?grossyfatties\.com
RewriteRule ^(.*)$ https://www.grossyfatties.com/$1 [R,L]
ErrorDocument 404 https://grossyfatties.com/error/404