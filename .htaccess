RewriteBase /
RewriteEngine On
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}.html -f
RewriteRule ^(.*)$ $1.html
RewriteCond %{SERVER_PORT} 80
RewriteCond %{HTTP_HOST} ^(www\.)?trippyfatties\.com
RewriteCond %{HTTP_HOST} ^(\.)?trippyfatties\.com
RewriteRule ^(.*)$ https://www.trippyfatties.com/$1 [R,L]
ErrorDocument 404 https://trippyfatties.com/error/404